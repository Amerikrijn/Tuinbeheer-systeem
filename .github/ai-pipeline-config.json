{
  "id": "ci-ai-pipeline",
  "name": "CI/CD AI Testing Pipeline",
  "description": "AI-powered testing pipeline for CI/CD - runs parallel to standard tests",
  "version": "2.0.0",
  "agents": [
    {
      "id": "test-generator",
      "name": "Test Generator Agent",
      "type": "test-generator",
      "status": "active",
      "path": "./agents/test-generator",
      "dependencies": [],
      "config": {
        "path": "./app/auth/login",
        "strategy": "risk-based",
        "maxInteractions": 100,
        "outputPath": "./agents/test-generator/test-results"
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000,
        "retries": 3
      }
    },
    {
      "id": "quality-analyzer",
      "name": "Quality Analyzer Agent",
      "type": "quality-analyzer",
      "status": "active",
      "path": "./agents/quality-analyzer",
      "dependencies": [],
      "config": {
        "path": "./app",
        "strategy": "comprehensive",
        "outputPath": "./agents/quality-analyzer/quality-results"
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000,
        "retries": 3
      }
    },
    {
      "id": "auto-fix",
      "name": "Auto-Fix Agent",
      "type": "auto-fix",
      "status": "active",
      "path": "./agents/auto-fix",
      "dependencies": ["test-generator", "quality-analyzer"],
      "config": {
        "filePath": "./app",
        "maxFixes": 25,
        "autoApply": false,
        "requireValidation": true,
        "includeSecurityFixes": true,
        "includePerformanceFixes": true,
        "includeQualityFixes": true,
        "outputPath": "./agents/auto-fix/auto-fix-results"
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000,
        "retries": 3
      }
    },
    {
      "id": "pipeline-orchestrator",
      "name": "Pipeline Orchestrator Agent",
      "type": "pipeline-orchestrator",
      "status": "active",
      "path": "./agents/pipeline-orchestrator",
      "dependencies": [],
      "config": {
        "workflowId": "ci-ai-pipeline",
        "parallel": true,
        "maxIterations": 2,
        "outputPath": "./agents/pipeline-orchestrator/orchestration-results"
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000,
        "retries": 3
      }
    },
    {
      "id": "ci-cd-pipeline",
      "name": "CI/CD Pipeline Agent",
      "type": "ci-cd",
      "status": "active",
      "path": "./",
      "dependencies": [],
      "config": {
        "buildCommand": "npm run build",
        "testCommand": "npm test",
        "outputPath": "./ci-cd-results"
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000,
        "retries": 3
      }
    },
    {
      "id": "ai-pipeline-v2",
      "name": "AI Pipeline v2.0 Agent",
      "type": "ai-pipeline",
      "status": "active",
      "path": "./agents/ai-pipeline-v2",
      "dependencies": [],
      "config": {
        "target": "./app",
        "iterations": 3,
        "quality": 85,
        "outputPath": "./agents/ai-pipeline-v2/ai-pipeline-results"
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000,
        "retries": 3
      }
    }
  ],
  "workflows": [
    {
      "id": "ci-ai-pipeline",
      "name": "CI/CD AI Testing Pipeline",
      "description": "Complete AI testing workflow for CI/CD",
      "steps": [
        {
          "id": "test-generation",
          "name": "Test Generation",
          "agentId": "test-generator",
          "order": 1,
          "required": true,
          "config": {
            "path": "./app/auth/login",
            "strategy": "risk-based",
            "maxInteractions": 100
          },
          "dependencies": [],
          "timeout": 300000,
          "retryPolicy": {
            "maxRetries": 2,
            "backoffMultiplier": 2,
            "initialDelay": 1000,
            "maxDelay": 10000
          },
          "rollbackPolicy": {
            "enabled": false,
            "strategy": "full",
            "checkpointInterval": 0,
            "maxCheckpoints": 0
          }
        },
        {
          "id": "quality-analysis",
          "name": "Quality Analysis",
          "agentId": "quality-analyzer",
          "order": 1,
          "required": true,
          "config": {
            "path": "./app",
            "strategy": "comprehensive",
            "outputPath": "./agents/quality-analyzer/quality-results"
          },
          "dependencies": [],
          "timeout": 180000,
          "retryPolicy": {
            "maxRetries": 2,
            "backoffMultiplier": 2,
            "initialDelay": 1000,
            "maxDelay": 10000
          },
          "rollbackPolicy": {
            "enabled": false,
            "strategy": "full",
            "checkpointInterval": 0,
            "maxCheckpoints": 0
          }
        },
        {
          "id": "auto-fixing",
          "name": "Auto-Fixing",
          "agentId": "auto-fix",
          "order": 2,
          "required": false,
          "config": {
            "filePath": "./app",
            "maxFixes": 25,
            "autoApply": false,
            "requireValidation": true,
            "includeSecurityFixes": true,
            "includePerformanceFixes": true,
            "includeQualityFixes": true,
            "outputPath": "./agents/auto-fix/auto-fix-results"
          },
          "dependencies": ["test-generation", "quality-analysis"],
          "timeout": 240000,
          "retryPolicy": {
            "maxRetries": 1,
            "backoffMultiplier": 2,
            "initialDelay": 2000,
            "maxDelay": 10000
          },
          "rollbackPolicy": {
            "enabled": true,
            "strategy": "partial",
            "checkpointInterval": 60000,
            "maxCheckpoints": 5
          }
        },
        {
          "id": "pipeline-orchestration",
          "name": "Pipeline Orchestration",
          "agentId": "pipeline-orchestrator",
          "order": 1,
          "required": true,
          "config": {
            "workflowId": "ci-ai-pipeline",
            "parallel": true,
            "maxIterations": 2,
            "outputPath": "./agents/pipeline-orchestrator/orchestration-results"
          },
          "dependencies": [],
          "timeout": 300000,
          "retryPolicy": {
            "maxRetries": 2,
            "backoffMultiplier": 2,
            "initialDelay": 1000,
            "maxDelay": 10000
          },
          "rollbackPolicy": {
            "enabled": false,
            "strategy": "full",
            "checkpointInterval": 0,
            "maxCheckpoints": 0
          }
        },
        {
          "id": "ci-cd-execution",
          "name": "CI/CD Execution",
          "agentId": "ci-cd-pipeline",
          "order": 1,
          "required": true,
          "config": {
            "buildCommand": "npm run build",
            "testCommand": "npm test",
            "outputPath": "./ci-cd-results"
          },
          "dependencies": [],
          "timeout": 300000,
          "retryPolicy": {
            "maxRetries": 2,
            "backoffMultiplier": 2,
            "initialDelay": 1000,
            "maxDelay": 10000
          },
          "rollbackPolicy": {
            "enabled": false,
            "strategy": "full",
            "checkpointInterval": 0,
            "maxCheckpoints": 0
          }
        },
        {
          "id": "ai-pipeline-execution",
          "name": "AI Pipeline Execution",
          "agentId": "ai-pipeline-v2",
          "order": 1,
          "required": true,
          "config": {
            "target": "./app",
            "iterations": 3,
            "quality": 85,
            "outputPath": "./agents/ai-pipeline-v2/ai-pipeline-results"
          },
          "dependencies": [],
          "timeout": 300000,
          "retryPolicy": {
            "maxRetries": 2,
            "backoffMultiplier": 2,
            "initialDelay": 1000,
            "maxDelay": 10000
          },
          "rollbackPolicy": {
            "enabled": false,
            "strategy": "full",
            "checkpointInterval": 0,
            "maxCheckpoints": 0
          }
        }
      ],
      "triggers": [
        {
          "type": "manual",
          "config": {},
          "enabled": true
        }
      ],
      "conditions": [
        {
          "type": "quality-score",
          "operator": "gte",
          "value": 60,
          "field": "qualityScore"
        }
      ],
      "maxRetries": 2,
      "timeout": 900000,
      "parallel": true,
      "enabled": true
    }
  ],
  "settings": {
    "maxConcurrentWorkflows": 6,
    "maxConcurrentAgents": 6,
    "defaultTimeout": 300000,
    "enableLogging": true,
    "enableMetrics": true,
    "enableNotifications": false,
    "parallelExecution": true,
    "agentTimeout": 300000,
    "workflowTimeout": 900000,
    "qualityGates": [
      {
        "id": "quality-threshold",
        "name": "Quality Score Threshold",
        "type": "quality-score",
        "threshold": 80,
        "operator": "gte",
        "action": "warn",
        "enabled": true
      },
      {
        "id": "test-coverage-threshold",
        "name": "Test Coverage Threshold",
        "type": "test-coverage",
        "threshold": 75,
        "operator": "gte",
        "action": "warn",
        "enabled": true
      },
      {
        "id": "agent-success-rate",
        "name": "Agent Success Rate",
        "type": "agent-success",
        "threshold": 90,
        "operator": "gte",
        "action": "block",
        "enabled": true
      },
      {
        "id": "parallel-execution-time",
        "name": "Parallel Execution Time",
        "type": "execution-time",
        "threshold": 300000,
        "operator": "lte",
        "action": "warn",
        "enabled": true
      }
    ],
    "notifications": []
  },
