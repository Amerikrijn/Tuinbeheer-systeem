# Copy to .env and fill in production credentials
# Used by scripts/run-sql-file.js to connect to your main database
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DATABASE?sslmode=require

# Optional: Supabase environment variables (already used elsewhere in app)
NEXT_PUBLIC_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR-ANON-KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR-SERVICE-ROLE-KEY

# Site Configuration  
NEXT_PUBLIC_SITE_URL=https://your-domain.com

# Emergency Admin Access (Banking Compliance)
NEXT_PUBLIC_EMERGENCY_ADMIN_EMAIL=admin@your-domain.com

# Protected Admin Email (Optional - for extra protection)
NEXT_PUBLIC_PROTECTED_ADMIN_EMAIL=admin@your-domain.com
