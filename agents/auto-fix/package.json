{
  "name": "auto-fix-agent",
  "version": "2.0.0",
  "description": "AI-powered Auto-Fix Agent with TypeScript compiler API, ESLint integration, ML-powered suggestions, and advanced code analysis",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node cli.ts",
    "dev": "ts-node --watch cli.ts",
    "test": "jest",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "analyze": "ts-node cli.ts --analyze-only",
    "fix": "ts-node cli.ts --auto-apply",
    "pr": "ts-node cli.ts --generate-pr",
    "ml:train": "ts-node scripts/train-ml-model.ts",
    "ml:predict": "ts-node scripts/ml-predict.ts"
  },
  "dependencies": {
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.1.0",
    "simple-git": "^3.22.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.1",
    "tensorflow": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "natural": "^6.10.4",
    "compromise": "^14.10.0",
    "acorn": "^8.11.0",
    "esprima": "^4.0.1",
    "recast": "^0.23.0",
    "jscodeshift": "^0.15.1",
    "glob": "^10.3.10",
    "minimatch": "^9.0.3",
    "yaml": "^2.3.4",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^3.0.8",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "ai",
    "testing",
    "code-quality",
    "auto-fix",
    "typescript-compiler",
    "eslint",
    "machine-learning",
    "code-analysis",
    "pull-request",
    "git-integration"
  ],
  "author": "AI Testing System",
  "license": "MIT"
}