# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Database Configuration
DATABASE_URL=your_database_connection_string_here

# Authentication
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Development Configuration
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# API Configuration
API_BASE_URL=http://localhost:3000/api
API_TIMEOUT=30000

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Logging
LOG_LEVEL=debug
ENABLE_AUDIT_LOGGING=true

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_DEBUG_MODE=true
ENABLE_PERFORMANCE_MONITORING=true

# External Services
STORAGE_BUCKET=your_storage_bucket_name
CDN_URL=your_cdn_url_here

# Test Configuration
TEST_DATABASE_URL=your_test_database_url_here
TEST_SUPABASE_URL=your_test_supabase_url_here
TEST_SUPABASE_ANON_KEY=your_test_supabase_anon_key_here